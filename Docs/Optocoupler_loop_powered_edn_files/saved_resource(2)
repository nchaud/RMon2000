                podcastPlayer.classList.add('is-poppedOut');
                podcastPage.classList.add('player-isStuck');
            } else {
                podcastPlayer.classList.remove('is-poppedOut');
                podcastPage.classList.remove('player-isStuck');
            }
        }

        lastScrollTop = st;
    }, false), 5000);

    function viewport() {
        var e = window,
            a = 'inner';

